{
    "name": "@hypermodel/docs",
    "version": "0.0.1",
    "type": "module",
    "main": "dist/index.js",
    "bin": {
        "docs": "./dist/index.js"
    },
    "scripts": {
        "build": "vite build",
        "start": "node dist/index.js",
        "start:worker": "node dist/temporal/worker.js",
        "inspect": "npx @modelcontextprotocol/inspector node dist/index.js",
        "publish": "npm publish --access public",
        "test:mcp": "npx tsx test_mcp.ts",
        "test:mcp:ci": "npm run build && npx tsx test_mcp.ts",
        "test:unit": "npx tsx src/deriveIndexName.test.ts",
        "lint": "eslint \"src/**/*.{js,ts}\"",
        "lint:fix": "eslint \"src/**/*.{js,ts}\" --fix",
        "format": "prettier --write \"src/**/*.{js,ts,json,md}\"",
        "format:check": "prettier --check \"src/**/*.{js,ts,json,md}\"",
        "prepare": "command -v husky >/dev/null 2>&1 && husky || exit 0"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hypermodel-labs/docs-mcp.git"
    },
    "author": "Hypermodel Labs",
    "description": "MCP server for enhanced documentation for AI coding agents",
    "dependencies": {
        "@anthropic-ai/claude-code": "^1.0.117",
        "@google/genai": "^1.16.0",
        "@modelcontextprotocol/sdk": "^1.17.3",
        "@temporalio/client": "^1.12.1",
        "@temporalio/worker": "^1.12.1",
        "@temporalio/workflow": "^1.12.1",
        "@workos-inc/node": "^7.63.0",
        "axios": "^1.8.1",
        "axios-cookiejar-support": "^6.0.4",
        "build": "^0.1.4",
        "cheerio": "^1.0.0-rc.12",
        "dashify": "^2.0.0",
        "detect-port": "^2.1.0",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "openai": "^5.18.1",
        "pdf-parse": "^1.1.1",
        "pg": "^8.12.0",
        "playwright": "^1.55.0",
        "temporalio": "^1.9.3",
        "tough-cookie": "^6.0.0",
        "zod": "^3.25.76"
    },
    "devDependencies": {
        "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
        "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
        "@eslint/js": "^9.31.0",
        "@types/dashify": "^1.0.3",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^5.0.1",
        "@types/minimist": "^1.2.5",
        "@types/node": "^22.13.5",
        "@types/pdf-parse": "^1.1.5",
        "@types/pg": "^8.11.10",
        "@types/react": "^18.2.64",
        "@types/react-dom": "^18.2.21",
        "@types/rollup": "^0.54.0",
        "@typescript-eslint/eslint-plugin": "^8.37.0",
        "@typescript-eslint/parser": "^8.37.0",
        "eslint": "^9.31.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-prettier": "^5.5.1",
        "events": "^3.3.0",
        "husky": "^9.1.7",
        "lint-staged": "^16.1.2",
        "prettier": "^3.6.2",
        "process": "^0.11.10",
        "rollup": "^4.37.0",
        "rollup-plugin-node-polyfills": "^0.2.1",
        "stream-browserify": "^3.0.0",
        "typescript": "^5.7.3",
        "vite": "^5.4.14",
        "vite-plugin-node": "^5.0.0",
        "vite-plugin-node-polyfills": "^0.23.0"
    },
    "files": [
        "dist"
    ],
    "lint-staged": {
        "*.{js,ts}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{json,md}": [
            "prettier --write"
        ]
    }
}
